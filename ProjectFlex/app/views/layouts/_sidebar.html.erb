<div class= "sidebar col-sm-2">
            <%= link_to( image_tag("logoWhite.png", class: "img-fluid"), "http://www.tamuflex.com") %>
    <div class="nav-sidebar", id= "sidebar">
            <%= link_to_unless_current "Home", root_path %>
        
            <%= link_to_unless_current "Calendar",
               {:controller => "member", :action => "calendar"} %>
       
            <%= link_to_unless_current "Forms",
               {:controller => "member", :action => "forms"} %>
    
            <%= link_to_unless_current "Marketplace",
               {:controller => "member", :action => "marketplace"} %>
            
            <%if session[:memType] != nil %>
               <% if session[:memType] == "Member" || session[:memType] == "Exec"%>
                  <%= link_to_unless_current "My Points",
                     {:controller => "member", :action => "mypoints"} %>
                  <% end %>
            <% end %>
            
            <%if session[:memType] != nil %>
               <% if session[:memType] == "Member" || session[:memType] == "Exec"%>
                  <%= link_to_unless_current "My Registrations",
                     {:controller => "member", :action => "myregistrations"} %>
               <% end %>
            <% end %>
            
            <%if session[:memType] != nil %>
               <% if session[:memType] == "Exec" || session[:memType] == "ZL" %>
                  <%= link_to_unless_current "Approve Points",
                  {:controller => "member", :action => "approvepoints"} %>
               <% end %>
            <% end %>
            
            <div data-turbolinks = "false">
               <%if session[:memType] != nil %>
                  <% if session[:memType] == "Exec" || session[:memType] == "ZL" %>
                     <%= link_to_unless_current "Manage Users",
                     {:controller => "member", :action => "manageusers"} %>
                  <% end %>
               <% end %>
            </div>
            
    </div>
</div>

<div id="users-device-size">
  <div id="xs" class="visible-xs"></div>
  <div id="sm" class="visible-sm"></div>
  <div id="md" class="visible-md"></div>
  <div id="lg" class="visible-lg"></div>
</div>

