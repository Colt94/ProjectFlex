<h1>Calendar</h1>
<p>A calendar of all upcoming events!</p>
<%= link_to "Home", "/" %>

<%-# Add variable to import fullCalendar options into script -%>
<div id="demo"></div>
<div id="calendar"></div>
<script type="text/javascript">
  $('#calendar').fullCalendar({events: 
      <%= @calendar_options.to_json.html_safe %>
  });
</script>
<% @events.each do |event| %>
Name: <%= event.name %>, Date: <%= event.date%>, Type: <%= event.point_type%>, Points: <%= event.point_value%>, ID: <%= event.id%></br>
<% end %></br>
